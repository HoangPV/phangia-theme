{% block html_head_container %}
	{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}
{% set body_class = body_class ~ ' ' ~ ['container-1340px', 'has-side-panel', 'side-panel-right']|join(' ') %}
<body class="{{body_class}}">
<!-- preloader -->
<div id="preloader">
	<div id="spinner">
		<div class="preloader-dot-loading">
			<div class="cssload-loading"><i></i><i></i><i></i><i></i></div>
		</div>
	</div>
	<div id="disable-preloader" class="btn btn-default btn-sm">{{ 'Disable Preloader'|t }}</div>
</div>
<div class="side-panel-body-overlay"></div>
<div id="side-panel-container" class="dark" data-tm-bg-img="{{ site.theme.link }}/images/side-push-bg.jpg">
	<div class="side-panel-wrap">
		<div id="side-panel-trigger-close" class="side-panel-trigger"><a href="#"><i class="fa fa-times side-panel-trigger-icon"></i></a></div>
		<img class="logo mb-50" src="{{ t_settings.get_logo }}" alt="{{ 'Logo'|t }}">
		<p>{{ t_settings.get_about_text }}</p>
		<div class="widget d-none">
			<h4 class="widget-title widget-title-line-bottom line-bottom-theme-colored1">Latest News</h4>
			<div class="latest-posts">
				<article class="post media-post clearfix pb-0 mb-10">
					<a class="post-thumb" href="#"><img src="http://placehold.it/95x95" alt=""></a>
					<div class="post-right">
						<h5 class="post-title mt-0"><a href="#">Sustainable Construction</a></h5>
						<p>Lorem ipsum dolor...</p>
					</div>
				</article>
				<article class="post media-post clearfix pb-0 mb-10">
					<a class="post-thumb" href="#"><img src="http://placehold.it/95x95" alt=""></a>
					<div class="post-right">
						<h5 class="post-title mt-0"><a href="#">Industrial Coatings</a></h5>
						<p>Lorem ipsum dolor...</p>
					</div>
				</article>
				<article class="post media-post clearfix pb-0 mb-10">
					<a class="post-thumb" href="#"><img src="http://placehold.it/95x95" alt=""></a>
					<div class="post-right">
						<h5 class="post-title mt-0"><a href="#">Storefront Installations</a></h5>
						<p>Lorem ipsum dolor...</p>
					</div>
				</article>
			</div>
		</div>

		<div class="widget">
			<h5 class="widget-title widget-title-line-bottom line-bottom-theme-colored1">{{ 'Thông tin liên hệ'}}</h5>
			<div class="tm-widget-contact-info contact-info-style1 contact-icon-theme-colored1">
				<ul>
					<li class="contact-name">
						<div class="icon"><i class="flaticon-contact-037-address"></i></div>
						<div class="text">{{ t_settings.get_founder_name }}</div>
					</li>
					<li class="contact-phone">
						<div class="icon"><i class="flaticon-contact-042-phone-1"></i></div>
						<div class="text"><a href="tel:{{ t_settings.get_phone }}">{{ t_settings.get_phone }}</a></div>
					</li>
					<li class="contact-email">
						<div class="icon"><i class="flaticon-contact-043-email-1"></i></div>
						<div class="text"><a href="mailto:{{ t_settings.get_email_address }}">{{ t_settings.get_email_address }}</a></div>
					</li>
					<li class="contact-address">
						<div class="icon"><i class="flaticon-contact-047-location"></i></div>
						<div class="text">{{ t_settings.get_address }}</div>
					</li>
				</ul>
			</div>
		</div>
	</div>
</div>
<div id="wrapper" class="clearfix">
	<!-- Header -->
	<header id="header" class="header header-layout-type-header-2rows">
		<div class="header-top">
			<div class="container">
				<div class="row">
					<div class="col-xl-auto header-top-left align-self-center text-center text-xl-left">
						<ul class="element contact-info">
							<li class="contact-phone"><i class="fa fa-phone font-icon sm-display-block"></i> {{ 'Tel'|t }}: {{ t_settings.get_phone }}</li>
							<li class="contact-email d-none"><i class="fa fa-envelope font-icon sm-display-block"></i> {{ t_settings.get_email_address }}</li>
							<li class="contact-address"><i class="fa fa-map font-icon sm-display-block"></i> {{ t_settings.get_address }}</li>
						</ul>
					</div>
					<div class="col-xl-auto ml-xl-auto header-top-right align-self-center text-center text-xl-right">
						<div class="element pt-0 pb-0">
							<ul class="styled-icons icon-dark icon-theme-colored1 icon-circled clearfix">
								<li><a class="social-link" href="{{ t_settings.get_facebook }}" ><i class="fab fa-facebook"></i></a></li>
								<li class="d-none"><a class="social-link" href="#" ><i class="fab fa-twitter"></i></a></li>
								<li class="d-none"><a class="social-link" href="#" ><i class="fab fa-youtube"></i></a></li>
							</ul>
						</div>
						<div class="element pt-0 pb-0">
							<a href="ajax-load/form-appointment.html" class="btn btn-theme-colored2 btn-flat btn-sm ajaxload-popup">Đặt hẹn</a>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="header-nav">
			{% block header %}
				{% include "menu.twig" with {'items': menu.get_items} %}
			{% endblock %}
		</div>
	</header>
	{% block content %}{% endblock %}

	<!-- Footer -->
	<footer id="footer" class="footer bg-no-repeat" data-tm-bg-img="images/footer-img1.png">
		<div class="footer-widget-area">
			<div class="container pt-100 pb-70">
				<div class="row">
					<div class="col-md-6 col-lg-6 col-xl-3">
						<div id="tm_widget_contact_info-1" class="split-nav-menu clearfix widget widget-contact-info clearfix mb-20">
							<div class="tm-widget tm-widget-contact-info contact-info contact-info-style1  contact-icon-theme-colored1">
								<h4 class="text-white">CÔNG TY TNHH ĐIỆN LẠNH PHAN GIA</h4>
								<p class="d-none">There are many variatio of lorem ipsum available.</p>
								<div class="footer-contact-address">
									<div class="phone mb-15">
										<h6 class="m-0 text-white"><i class="fas fa-phone-alt text-theme-colored1 mr-10"></i>{{ t_settings.get_phone }}</h6>
									</div>
									<div class="email mb-15">
										<h6 class="m-0 text-white"><i class="fas fa-envelope text-theme-colored1 mr-10"></i>{{ t_settings.get_email_address }}</h6>
									</div>
									<div class="address">
										<h6 class="m-0 text-white"><i class="fas fa-map text-theme-colored1 mr-10"></i>{{ t_settings.get_address }}</h6>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="col-md-6 col-lg-6 col-xl-3">
						<div id="nav_menu-1" class="widget widget_nav_menu">
							<h4 class="widget-title">Khám phá</h4>
							<div class="menu-footer-page-list">
								<ul id="footer-page-list" class="menu">
									{% if footer_menu %}
										{% for item in footer_menu.items %}
											<li><a href="{{ item.url }}">{{ item.name }}</a></li>
										{% endfor %}
									{% endif %}

									{#<li><a href="#">Privacy Policy</a></li>
									<li><a href="#">Terms of use</a></li>#}
								</ul>
							</div>
						</div>
					</div>
					<div class="col-md-6 col-lg-6 col-xl-3">
						<div id="tm_widget_blog_list-1" class="split-nav-menu clearfix widget widget-blog-list clearfix">
							<h4 class="widget-title mb-40">Tin mới nhất</h4>
							<div class="tm-widget tm-widget-blog-list ">
								<!-- the loop -->
								<article class="post media-post clearfix mb-30">
									<a class="post-thumb" href="#"><img width="100" height="70" src="http://placehold.it/95x95" class="" alt="" /></a>
									<div class="post-right">
                    <span class="post-date">
                    <time class="entry-date text-theme-colored1 mb-10" datetime="2020-05-15T06:10:26+00:00">April 15, 2020</time>
                    </span>
										<h6 class="post-title"> <a href="#">We provide all types of roofing services </a> </h6>
									</div>
								</article>
								<article class="post media-post clearfix">
									<a class="post-thumb" href="#"><img width="100" height="70" src="http://placehold.it/95x95" class="" alt="" /></a>
									<div class="post-right">
                    <span class="post-date">
                    <time class="entry-date text-theme-colored1 mb-10" datetime="2020-05-15T06:10:26+00:00">April 15, 2020</time>
                    </span>
										<h6 class="post-title"> <a href="#">We provide all types of roofing services </a> </h6>
									</div>
								</article>
								<!-- end of the loop -->
							</div>
						</div>
					</div>
					<div class="col-md-6 col-lg-6 col-xl-3">
						<div class="tm-widget tm-widget-opening-hours tm-widget-opening-hours-compressed opening-hours border-dark">
							<h4 class="widget-title text-white">Giờ mở cửa</h4>
							<ul class="opening-hours p-0">
								<li class="clearfix">
									<span>Chúng tôi mở cửa vào Thứ 2 đến Thứ 7 hàng tuần</span>
									<div class="value">8.00 - 17.00</div>
								</li>
								<li class="clearfix d-none">
									<span>Wednesday</span>
									<div class="value">9.00 - 16.00</div>
								</li>
								<li class="clearfix d-none">
									<span>Thursday - Friday</span>
									<div class="value">9.00 - 16.00</div>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
			<div class="footer-bottom">
				<div class="container">
					<div class="row pt-40 pb-40">
						<div class="col"></div>
						<div class="col-sm-6">
							<div class="footer-paragraph text-center">
								© 2021 Kenhana. Đã đăng ký bản quyền.
							</div>
						</div>
						<div class="col"></div>
					</div>
				</div>
			</div>
		</div>
	</footer>
	<a class="scrollToTop" href="#"><i class="fa fa-angle-up"></i></a>
</div>
{% block footer %}
	<footer id="footer">
		{% include 'footer.twig' %}
	</footer>
	{{ function('wp_footer') }}
{% endblock %}
<script src="{{ site.theme.link }}/js/custom.js"></script>
</body>
</html>
